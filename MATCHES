#include <iostream>
using namespace std;

int main()
{
    int n=0,A=0,B=0,m=0,a=0;
    int arr[10]={6,2,5,5,4,5,6,3,7,6};
    cin>>n;
    for(int j=0;j<n;j++)
    {
    cin>>A;
    cin>>B;
    int num=A+B;
    int sum=0;
    for (int i = num; num > 0; num= num / 10) 
    {
        a= num % 10;
        sum+=arr[a];
    }
      cout<<sum<<endl;
    }
    return 0;
}
