#include<iostream>
using namespace std;

int main()
{
	int a,b;
	cin >> a >> b;
	
	int arr[a][b];
	
	for(int i = 0; i < a; i++)
	{
		for(int j = 0; j < b; j++)
		{
			cin >> arr[i][j];
		}
	}
	
	for(int l = 0; l < a; l++)
	{
		for(int k = 0; k < b; k++)
		{
			if(l==0  || l ==(a-1) || (l+k)==(a-1) )
			{
				cout << arr[l][k];
			}
		}
	}
}
